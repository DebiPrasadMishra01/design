<!-- JavaScript with custom speed -->
<script>
function smoothScrollToTop(speed = 500) {
const scrollStep = -window.scrollY / (speed / 10);
const scrollInterval = setInterval(() => {
if (window.scrollY !== 0) {
window.scrollBy(0, scrollStep);
} else {
clearInterval(scrollInterval);
}
}, 10); // 10ms interval
}
</script>

 

